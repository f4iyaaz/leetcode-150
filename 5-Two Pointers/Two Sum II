class Solution {
public:
    vector<int> twoSum(vector<int>& numbers, int target) {
        map<int, int> m;
    vector<int> result;
    for(int i = 0; i < numbers.size(); i++) {
        int value_to_look = target - numbers[i];
        if(!m.empty() && m.count(value_to_look) > 0) {
            result.push_back(m[value_to_look] + 1);
            result.push_back(i + 1);
            break;
        }
        else {
            m[numbers[i]] = i;
        }
    }
    return result;
    }
};
